############################################################

protocol example_protocol

base generic_base

include "../basic_stuff"

include "communication"

############################################################

obj SomeObject

    uint8 is_ready
    int16 min
    int16 max

    int16 a : (10, 15)
    int16 b : (10, 15]
    int16 c : [10, 15)
    int16 d : [10, 15]
    int16 e : (10,)
    int16 f : [10,)
    int16 g : (, 15)
    int16 h : (, 15]
    int16 i : [0.5, 1.5]

    bool  is_enabled

    float factor : (, 2.0]

    double price : (0,)

    string name

    string id : (1,)

    string product_description : [0,256)

obj_end

############################################################

obj EmptyObject

obj_end

############################################################

obj AnotherObject

    int16 id

    o SomeObject dummy

    o EmptyObject empty

    e Color       color

obj_end

############################################################
